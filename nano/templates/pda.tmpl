
<!--
Title: PDA UI
Used In File(s): \code\game\objects\items\devices\PDA\PDA.dm
 -->

{{if data.owner}}
	<div class="Statusbar">
	<div class="floatLeft">
		<div class="itemLabelWide">
			 <b>Горячие клавиши</b>:
        </div>
        <div class="itemContent">
			<!--{{:helper.link('Refresh', 'refresh', {'choice' : "Refresh"}, null, 'fixedLeft')}}-->
			{{:helper.link('Выйти', 'gear', {'choice' : "Close"}, null, 'fixedLeft')}}
			{{if data.idInserted}} {{:helper.link('Перезагрузить', 'eject', {'choice' : "UpdateInfo"}, null, 'fixedLeftWide')}} {{/if}}
			{{if data.mode != 0}} {{:helper.link('Назад', 'arrowreturn-1-w', {'choice' : "Return"}, null, 'fixedLeft')}} {{/if}}
		</div>
	</div>
	<div class="floatRight">
		<span class="average">{{:data.stationTime}}&emsp;[{{:data.securityLevel}}]</span>
	</div>
    {{if data.shuttle_eta}}
        <div class="item">
            <div class="itemLabelNarrow">
                <b>Прибытие шаттла</b>:
            </div>
            <div class="itemContent">
                <span class="average">{{:data.shuttle_eta}}</span>
            </div>
        </div>
    {{/if}}
	</div>
    


	{{if data.mode == 0}} <!-- Main Menu -->
		<div class="Main_header">
		<br>
        <div class="floatLeft">
            <div class="itemLabelNarrow">
                <b>Аккаунт</b>:
            </div>
            <div class="itemContent">
                <span class="average">{{:data.owner}}, {{:data.ownjob}}</span>
            </div>
        </div>
        <div class="floatRight">
            <div class="itemLabelNarrow">
                <b>ИН</b>:
            </div>
            <div class="itemContent">
                {{:helper.link(data.idLink, 'eject', {'choice' : "Authenticate"}, data.idInserted ? null : 'disabled', data.idInserted ? 'floatright' : 'fixedLeft')}}
            </div>
        </div>
        <br>
        <div class="item">
            <div class="itemLabelNarrow">
                    <b>Доп. Устройства</b>:
            </div>
            <div class="itemContent">
                {{if data.cart_loaded==1}}
                    {{:helper.link(data.cartridge.name, 'eject', {'choice' : "Eject"},null,null)}}
                {{else}}
                    {{:helper.link('Слот пуст', 'eject', {'choice' : "Eject"},'disabled',null)}}
                {{/if}}
            </div>
        </div>
		</div>
        <div class="item">
            <H2>Приложения</H2>
        </div>
        <div class="item">
            <div class="itemLabelNarrow">
                <b>Стандартные</b>:
            </div>
            <div class="itemContent">
                {{:helper.link('Блокнот', 'note', {'choice' : "1"}, null, 'fixedLeftWide')}}
                {{:helper.link('Почта', data.new_Message ? 'mail-closed' : 'mail-open', {'choice' : "2"}, null, 'fixedLeftWide')}}
                {{:helper.link('Экипаж', 'contact', {'choice' : "41"}, null, 'fixedLeftWide')}}
				<!-- {{:helper.link('News', data.new_News ? 'mail-closed' : 'mail-open', {'choice' : "6"}, null, 'fixedLeftWide')}} -->
				{{:helper.link('Кошелёк', 'suitcase', {'choice' : "Finance"}, null, 'fixedLeftWide')}}
            </div>
        </div>
        <br>
        {{if data.cartridge}}
            {{if data.cartridge.access.access_clown == 1}}
                <div class="item">
                    <div class="itemLabelNarrow">
                        <b>Хонкер</b>:
                    </div>
                    <div class="itemContent">
                        {{:helper.link('Генератор хонка', 'gear', {'choice' : "Honk"}, null, 'fixedLeftWide')}}
                    </div>
                </div>
                <br>
            {{/if}}
            {{if data.cartridge.access.access_engine == 1}}
                <div class="item">
                    <div class="itemLabelNarrow">
                        <b>Инженерия</b>:
                    </div>
                    <div class="itemContent">
                        {{:helper.link('Электросети', 'alert', {'choice' : "43"}, null, 'fixedLeftWide')}}
                    </div>
                </div>
                 <br>
            {{/if}}
            {{if data.cartridge.access.access_medical == 1}}
                <div class="item">
                    <div class="itemLabelNarrow">
                        <b>Медицина</b>:
                    </div>
                    <div class="itemContent">
                            {{:helper.link('Мед. записи', 'gear', {'choice' : "44"}, null, 'fixedLeftWide')}}
                            {{:helper.link(data.scanmode == 1 ? 'Выкл. мед. сканер' : 'Вкл. мед. сканер', 'gear', {'choice' : "Medical Scan"}, null , 'fixedLeftWide')}}
                    </div>
                </div>
                <br>
            {{/if}}
            {{if data.cartridge.access.access_security == 1}}
                <div class="item">
                    <div class="itemLabelNarrow">
                        <b>Охрана</b>:
                    </div>
                    <div class="itemContent">
                        {{:helper.link('Охран. записи', 'gear', {'choice' : "45"}, null, 'fixedLeftWide')}}
                        {{if data.cartridge.radio ==1}} {{:helper.link('Управление ботом охраны', 'gear', {'choice' : "46"}, null, 'fixedLeftWide')}} {{/if}}
                        <br>
                    </div>
                </div>
                <br>
            {{/if}}
            {{if data.cartridge.access.access_quartermaster == 1}}
                <div class="item">
                    <div class="itemLabelNarrow">
                        <b>Логистика</b>:
                    </div>
                    <div class="itemContent">
                        {{:helper.link('Груз. записи', 'gear', {'choice' : "47"}, null, 'fixedLeftWide')}}
                        {{if data.cartridge.radio == 3}} {{:helper.link('Управление ботом доставки', 'gear', {'choice' : "48"}, null, 'fixedLeftWide')}} {{/if}}
                         <br>
                    </div>
                </div>
                <br>
            {{/if}}
        {{/if}}
        <br>
        <div class="item">
            <div class = "itemLabelNarrow">
                <b>Утилиты</b>:
            </div>
            <div class = "itemContent">
                {{if data.cartridge}}
                    {{if data.cartridge.access.access_status_display == 1}}
                        {{:helper.link('Мониторы станции', 'gear', {'choice' : "42"}, null, 'fixedLeftWide')}}
                    {{/if}}
                    {{if data.cartridge.access.access_janitor==1}}
                        {{:helper.link('Инв. уборщика', 'gear', {'choice' : "49"}, null, 'fixedLeftWide')}}
                    {{/if}}
                    {{if data.cartridge.radio == 2}}
                        {{:helper.link('Радиопередатчик', 'gear', {'choice' : "40"}, null, 'fixedLeftWide')}}
                    {{/if}}
                    {{if data.cartridge.access.access_reagent_scanner==1}}
                        {{:helper.link(data.scanmode == 3 ? 'Выкл. спектрометр' : 'Вкл. спектрометр', 'gear', {'choice' : "Reagent Scan"}, null, 'fixedLeftWide')}}
                    {{/if}}
                    {{if data.cartridge.access.access_engine==1}}
                        {{:helper.link(data.scanmode == 4 ? 'Выкл. дозиметр' : 'Вкл. дозиметр', 'gear', {'choice' : "Halogen Counter"}, null, 'fixedLeftWide')}}
                    {{/if}}
                    {{if data.cartridge.access.access_atmos==1}}
                        {{:helper.link(data.scanmode == 5 ? 'Выкл. газоанализатор' : 'Вкл. газоанализатор', 'gear', {'choice' : "Gas Scan"}, null, 'fixedLeftWide')}}
                    {{/if}}
                    {{if data.cartridge.access.access_remote_door==1}}
                        {{:helper.link('Управление шлюзами', 'gear', {'choice' : "Toggle Door"}, null, 'fixedLeftWide')}}
                    {{/if}}
                {{/if}}
                {{:helper.link('Газоанализатор', 'gear', {'choice' : "3"}, null, 'fixedLeftWide')}}
                {{:helper.link(data.fon==1 ? 'Выкл. фонарик' : 'Вкл. фонарик', 'lightbulb', {'choice' : "Light"}, null,'fixedLeftWide')}}
            </div>
        </div>
        {{if data.pai}}
            <div class="item">
                <div class = "itemLabelNarrow">
                    <b>Управление ПИИ</b>:
                </div>
                <div class = "itemContent">
                    {{:helper.link('Настройки', 'gear', {'choice' : "pai", 'option' : "1"}, null, 'fixedLeft')}}
                    {{:helper.link('Извлечь', 'eject', {'choice' : "pai", 'option' : "2"}, null, 'fixedLeft')}}
                </div>
            </div>
        {{/if}}


	{{else data.mode == 1}} <!-- Notekeeper -->
		<div class="Note">
			<div class="Note_header">
				<center><H3><span class="dark"><b>Блокнот</b>:</span></H3></center>
			</div>
			{{:data.note}}<br>
			{{:helper.link('Редактировать', 'pencil', {'choice' : "Edit"}, null, 'floatRight')}}<br>
		</div>


    {{else data.mode == 2}} <!-- Messenger -->
		<div class="Chat_header" style="overflow: hidden;">
			<div class="Chat_logo">
				<center><H2>Почта</H2></center>
			</div>
        <div class="item">
            <div class="itemLabelNarrow">
                <center><b>Параметры</b>:</center>
            </div>
            <div class ="itemContent" style="position: relative; left: 50px;">
                {{:helper.link(data.message_silent==1 ? 'Режим: Звук' : 'Режим: Без звука', data.message_silent==1 ? 'volume-off' : 'volume-on', {'choice' : "Toggle Ringer"}, null, 'fixedLeftWide')}}
                {{:helper.link(data.toff==1 ? 'Статус: Не в сети' : 'Статус: В сети',data.toff==1 ? 'close':'check', {'choice' : "Toggle Messenger"}, null, 'fixedLeftWide')}}
                {{:helper.link('Изменить рингтон', 'comment', {'choice' : "Ringtone"}, null, 'fixedLeftWide')}}
                {{:helper.link('Удалить все чаты', 'trash', {'choice' : "Clear", 'option' : "All"}, null, 'fixedLeftWide')}}
            </div>
        </div>
        {{if data.toff == 0}}
            {{if data.cartridge}}
                {{if data.cartridge.charges}}
                    <div class="item">
                        <b>{{:data.cartridge.charges}}
                        {{if data.cartridge.access.access_detonate_pda}} зарядов осталось. {{/if}}
                        {{if data.cartridge.access.access_clown || data.cartridge.access.access_mime}} троянов осталось. {{/if}}
                        </b>
                    </div>
                {{/if}}
            {{/if}}

            {{if data.pda_count == 0}}
                <b>Нет других КПК</b>
            {{else}}
                <center><H3>Активные чаты</H3></center>
                {{for data.convopdas}}
					<div class="Chat" style="overflow: auto;">
                    <div class="item">
                        {{:helper.link(value.Name, 'circle-arrow-s', {'choice' : "Select Conversation", 'convo' : value.Reference } , null, value.fixedLeftWider)}}
                        {{if data.cartridge}}
                            {{if data.cartridge.access.access_detonate_pda && value.Detonate}}
                                {{:helper.link('*Взорвать*', 'radiation', {'choice' : "Detonate", 'target' : value.Reference}, null, 'fixedLeft')}}
                            {{/if}}
                            {{if data.cartridge.access.access_clown}}
                                {{:helper.link('*Отправить вирус*', 'star', {'choice' : "Send Honk", 'target' : value.Reference}, null, 'fixedLeft')}}
                            {{/if}}
                            {{if data.cartridge.access.access_mime}}
                                {{:helper.link('*Отправить вирус*', 'circle-arrow-s', {'choice' : "Send Silence", 'target' : value.Reference}, null, 'fixedLeft')}}
                            {{/if}}
                        {{/if}}
                    </div>
					</div>
                {{/for}}
                <center><H3>Создать новый чат</H3><center>
                {{for data.pdas}}
					<div class="Chat" style="overflow: auto;">
                    <div class="item">
                        {{:helper.link(value.Name, 'circle-arrow-s', {'choice' : "Message", 'target' : value.Reference}, null, value.fixedLeftWider)}}
                        {{if data.cartridge}}
                            {{if data.cartridge.access.access_detonate_pda && value.Detonate}} {{:helper.link('*Взорвать*', 'radiation', {'choice' : "Detonate", 'target' : value.Reference}, null, 'fixedLeft')}}  {{/if}}
                            {{if data.cartridge.access.access_clown}} {{:helper.link('*Отправить вирус*', 'star', {'choice' : "Send Honk", 'target' : value.Reference}, null, 'fixedLeft')}}  {{/if}}
                            {{if data.cartridge.access.access_mime}}  {{:helper.link('*Отправить вирус*', 'circle-arrow-s', {'choice' : "Send Silence", 'target' : value.Reference}, null, 'fixedLeft')}}  {{/if}}
                        {{/if}}
                    </div>
					</div>
                {{/for}}
            {{/if}}
        {{/if}}
		</div>


	{{else data.mode == 21}} <!-- Messenger -->
	    <div class="Chat_header" style="overflow: hidden;">
			<div class="Chat_logo">
				<center><H2>Почта</H2></center>
			</div>
			<div class="item"><H3><span class="Chat_header_buttons_chatname">Чат с:&nbsp;<span class="average">{{:data.convo_name}}&nbsp;({{:data.convo_job}})</span></span>
			<small>{{:helper.link('Очистить историю сообщений', 'trash', {'choice' : "Clear", 'option' : "Convo"}, null, 'Chat_header_buttons_clear')}}</small></H3></div>
			<div class="Chat" style="overflow: auto;">
				<div class="itemContent" style="width: 100%;">
					{{for data.messages}}
						{{if data.active_conversation == value.target}}
							{{if value.sent==0}}
								<span class="message_left"><small><span class="average">{{:value.reciever}}</span></small><br>{{:value.message}}</span><br>
							{{else}}
								<span class="message_right">{{:value.message}}</span><br>
							{{/if}}
						{{/if}}
					{{/for}}
				</div>
			</div>
			<center>{{:helper.link('Ответить', 'comment', {'choice' : "Message", 'target': data.active_conversation}, null, 'new_message')}}</center>
		</div>


	{{else data.mode== 41}} <!-- Manifest uses cached data so we only use nanowriter when shit changes.-->
	    <div class="item">
            <center><table class="pmon"><tbody>
                {{if data.cached.heads.length}}
                    <tr><th colspan="4" class="command">Командный состав</th></tr>
                    {{for data.cached["heads"]}}
                        {{if value.rank == "Captain"}}
							<tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
							<tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.sec.length}}
                    <tr><th colspan="4" class="sec">Охранный состав</th></tr>
                    {{for data.cached["sec"]}}
                        {{if value.rank == "Head of Security"}}
							<tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
							<tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.eng.length}}
                    <tr><th colspan="4" class="eng">Инженерный персонал</th></tr>
                    {{for data.cached["eng"]}}
                        {{if value.rank == "Chief Engineer"}}
                            <tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
                            <tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.med.length}}
                    <tr><th colspan="4" class="med">Медицинский персонал</th></tr>
                    {{for data.cached["med"]}}
                        {{if value.rank == "Chief Medical Officer"}}
                            <tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
                            <tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.sci.length}}
                    <tr><th colspan="4" class="sci">Научный персонал</th></tr>
                    {{for data.cached["sci"]}}
                        {{if value.rank == "Research Director"}}
                            <tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
                            <tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.civ.length}}
                    <tr><th colspan="4" class="civ">Гражданский персонал</th></tr>
                    {{for data.cached["civ"]}}
                        {{if value.rank == "Head of Personnel"}}
                            <tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="good">{{:value.name}}</span></td><td><span class="good">{{:value.rank}}</span></td><td><span class="good">{{:value.active}}</span></td></tr>
                        {{else}}
                            <tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                            <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                        {{/if}}
                    {{/for}}
                {{/if}}
                {{if data.cached.misc.length}}
                    <tr><th colspan="4" class="misc">Другое</th></tr>
                    {{for data.cached["misc"]}}
                        <tr><td>{{:helper.link('Перевод на карту', null, {'choice' : "Send Money", 'account' : value.account}, null, null)}}{{:helper.link('Чат', null, {'choice' : "DM", 'reciever' : value.name}, null, null)}}</td>
                        <td><span class="average">{{:value.name}}</span></td><td><span class="average">{{:value.rank}}</span></td><td><span class="average">{{:value.active}}</span></td></tr>
                    {{/for}}
                {{/if}}
            </tbody></table></center>
        </div>


	{{else data.mode == 3}}
        <H2>Газоанализатор</H2>
        <div class="statusDisplay" style="height: 250px; overflow: auto;">
            <div class="item">
                {{if data.aircontents.reading == 1}}
                    <div class="itemLabel">
                        Давление:
                    </div>
                    <div class = "itemContent">
				        {{:helper.string('<span class="{0}">{1} кПа</span>', data.aircontents.pressure < 80 || data.aircontents.pressure > 120 ? 'bad' : data.aircontents.pressure < 95 || data.aircontents.pressure > 110 ? 'average' : 'good' , data.aircontents.pressure)}}
                    </div>
                    <div class="itemLabel">
                        Температура:
                    </div>
                    <div class = "itemContent">
				        {{:helper.string('<span class="{0}">{1} &deg;C</span>', data.aircontents.temp < 5 || data.aircontents.temp > 35 ? 'bad' : data.aircontents.temp < 15 || data.aircontents.temp > 25 ? 'average' : 'good' , data.aircontents.temp)}}
                    </div>
                    <br>
                    <div class="itemLabel">
                        Кислород:
                    </div>
                    <div class = "itemContent">
                        {{:helper.string('<span class="{0}">{1}%</span>', data.aircontents.oxygen < 17 ? 'bad' : data.aircontents.oxygen < 19 ? 'average' : 'good' , data.aircontents.oxygen)}}
                    </div>
                    <div class="itemLabel">
                        Азот:
                    </div>
                    <div class = "itemContent">
                        {{:helper.string('<span class="{0}">{1}%</span>', data.aircontents.nitrogen > 82 ? 'bad' : data.aircontents.nitrogen > 80 ? 'average' : 'good' , data.aircontents.nitrogen)}}
                    </div>
                    <div class="itemLabel">
                        Углекислый газ:
                    </div>
                    <div class = "itemContent">
                        {{:helper.string('<span class="{0}">{1}%</span>', data.aircontents.carbon_dioxide > 5 ? 'bad' : 'good' , data.aircontents.carbon_dioxide)}}
                    </div>
                    <div class="itemLabel">
                        Форон:
                    </div>
                    <div class = "itemContent">
                        {{:helper.string('<span class="{0}">{1}%</span>', data.aircontents.phoron  > 0 ? 'bad' : 'good' , data.aircontents.phoron)}}

                    </div>
                    {{if data.aircontents.other > 0}}
                        <div class="itemLabel">
                            Неизвестный газ:
                        </div>
                        <div class = "itemContent">
                            <span class="bad">{{:data.aircontents.other}}%</span>
                        </div>
                    {{/if}}
